#include <iostream>
using namespace std;
int main() {
int N;
int fare;
int totalFare = 0;
cout << "Enter number of students: ";
cin >> N;
cout << "Enter monthly fare per student: ";
cin >> fare;
if(fare < 1000) {
 cout << "Subsidized";
}
for (int i = 1; i <= N; i++) {
totalFare += fare;
}
cout << "Total fare collected: " << totalFare ;
return 0;
}
